{
    "presets": [
      [
        "next/babel",
        {
          "preset-react": {
            "runtime": "automatic",
            "importSource": "@emotion/react"
          }
        }
      ]
    ],
    
    "plugins":[ 
      ["@emotion/babel-plugin"],
    [
      "emotion",
      {
       "autoLabel": true,
       "labelFormat": "[dirname]-[filename]--[local]",
      }
    ]]
  }